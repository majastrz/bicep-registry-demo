{
  "cloud": {
    "currentProfile": "AzureCloud",
    "credentialPrecedence": [
      "AzureCLI",
      "AzurePowerShell"
    ]
  },
  "analyzers": {
    "core": {
      "enabled": true,
      "verbose": false,
      "rules": {
        "no-unused-params": {
          "level": "off"
        },
        "no-unused-vars": {
          "level": "off"
        },
        "no-hardcoded-env-urls": {
          "level": "warning"
        },
        "no-loc-expr-outside-params": {
          "level": "off"
        }
      }
    }
  },
  "experimentalFeaturesEnabled": {
    "paramsFiles": true
  }
}