{
  "cloud": {
    "currentProfile": "AzureCloud",
    "credentialPrecedence": [
      "AzureCLI",
      "AzurePowerShell"
    ]
  },
  "moduleAliases": {
    "ts": {},
    "br": {
      "test": {
        "registry": "majastrzoci.azurecr.io"
      }
    }
  },
  "analyzers": {
    "core": {
      "enabled": true,
      "verbose": false,
      "rules": {
        "no-hardcoded-location": {
          "level": "off"
        },
        "no-unused-params": {
          "level": "off"
        },
        "no-unused-vars": {
          "level": "off"
        },
        "no-hardcoded-env-urls": {
          "level": "warning"
        },
        "no-loc-expr-outside-params": {
          "level": "off"
        }
      }
    }
  },
  "experimentalFeaturesEnabled": {
  }
}